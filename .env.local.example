# General runtime configuration
CACHE_SECONDS=3600
ENABLE_WRITE=true
APP_TZ=America/Chicago

# Admin API secret for triggering precompute jobs
ADMIN_PRECOMPUTE_TOKEN=

# College football schedule provider configuration
# Supported providers: cfbd (default) or espn
CFB_SCHEDULE_PROVIDER=cfbd
# Required when using CFBD provider
CFBD_API_KEY=

# Vercel KV configuration (optional)
KV_REST_API_URL=
KV_REST_API_TOKEN=
KV_REST_API_READ_ONLY_TOKEN=

# Vercel Blob fallback configuration (optional)
BLOB_READ_WRITE_TOKEN=
BLOB_URL=

# Optional override for NFLverse fetch behaviour
NFLVERSE_CACHE_DIR=
NFLVERSE_FETCH_TIMEOUT_MS=
NFLVERSE_FETCH_ATTEMPTS=
